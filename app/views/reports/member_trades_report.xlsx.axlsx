records= report.results[:trades]
columns = %i[created_at id market taker_type seller sell_order seller_fee buyer buy_order buyer_fee taker_fee maker_fee amount price total]
xlsx_package.workbook.add_worksheet(name: report.name) do |sheet|
  render 'present_form', sheet: sheet, form: report.form_object

  sheet.add_row columns
  records.each do |record|
    sheet.add_row columns.map { |c| record.send c }
  end
end
